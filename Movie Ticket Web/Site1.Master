        header {
            background-color: #000;
            color: #fff;
            display: flex;
            justify-content: space-between;
            align-items: center;
            position: sticky;
            top: 0;
            z-index: 100;
        }

        .leftnav,
        .rightnav {
            display: flex;
            list-style: none;
            align-items: center;
        }

            .leftnav a,
            .rightnav a {
                text-decoration: none;
                color: #999;
            }

        .leftnav {
            padding-left: 10px;
        }

        .rightnav {
            padding-right: 10px;
        }

        .leftnav img {
            width: 150px;
        }

        .leftnav li,
        .rightnav li {
            padding: 3px 15px;
        }

        .leftnav a:hover,
        .rightnav a:hover {
            color: #fff;
        }

        header .hov::after {
            content: "";
            width: 0%;
            height: 2px;
            background: #fff;
            display: block;
            margin: auto;
            transition: 0.5s;
        }

        header .hov:hover::after {
            width: 100%;
        }
        footer {
    background-color: #000;
    color: #fff;
}

    footer a {
        text-decoration: none;
        transition: .5s;
        color: #fff;
