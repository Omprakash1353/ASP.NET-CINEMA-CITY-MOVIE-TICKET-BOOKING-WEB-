    }

        .social-media ul li a {
            display: inline-block;
            margin-right: 50px;
            height: 50px;
            width: 50px;
            padding-top: 12px;
            background-color: transparent;
            border: 1px solid #fff;
            text-align: center;
        }

            .social-media ul li a:hover {
                background-color: #fff;
                color: #000;
            }

.link ul li a {
